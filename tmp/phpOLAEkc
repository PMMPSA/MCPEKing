		parent::__construct(
			$name,
			"%pocketmine.command.me.description",
			"%pocketmine.command.me.usage"
		);
		$this->setPermission("pocketmine.command.me");
	}

	/**
	 * @param CommandSender $sender
	 * @param string        $currentAlias
