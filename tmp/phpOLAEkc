class RespawnPacket extends DataPacket {

	const NETWORK_ID = Info::RESPAWN_PACKET;

	public $x;
	public $y;
	public $z;

	public function decode(){
		$this->getVector3f($this->x, $this->y, $this->z);
	}

	public function encode(){
		$this->reset();
		$this->putVector3f($this->x, $this->y, $this->z);
	}

}<?php

