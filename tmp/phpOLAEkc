use pocketmine\block\BlockIds;

interface ItemIds extends BlockIds {

	//All BlockIds are also accessible through this interface.
	const IRON_SHOVEL = 256;
	const IRON_PICKAXE = 257;
	const IRON_AXE = 258;
	const FLINT_AND_STEEL = 259;
